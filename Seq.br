unit Seq

defn Seq.range:
  my start; end
  yields (end - start)
  do start < end:
    yields start
    start = start + 1

defn Seq.map:
  my function; ln
  do ln > 0:
    my a
    yields (function a)
    ln = ln - 1

lazy Seq.seq:
  my ln
  i = 0
  do i < ln:
    my a
    i = i + 1
  my _
  i = 0
  yields ln
  do i < ln:
    yields (rc (1 + i))
    i = i + 1